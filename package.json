{
  "name": "cdk8s-simple-webapp",
  "version": "0.0.1",
  "description": "A library for describing simple webapps on Kubernetes",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/toricls/cdk8s-simple-webapp.git"
  },
  "author": "Tori <yshr@amazon.com>",
  "license": "Apache-2.0",
  "scripts": {
    "compile": "jsii",
    "watch": "jsii -w",
    "test": "echo ok",
    "compat": "npx jsii-diff npm:$(node -p \"require('./package.json').name\")",
    "package": "jsii-pacmak",
    "bump": "standard-version",
    "build": "yarn compile && yarn test && yarn run package"
  },
  "dependencies": {
    "cdk8s": "^0.20.0",
    "constructs": "2.0.1"
  },
  "devDependencies": {
    "jsii": "^1.5.0",
    "jsii-diff": "^1.5.0",
    "jsii-pacmak": "^1.5.0",
    "jsii-release": "^0.1.5",
    "standard-version": "^8.0.0"
  },
  "stability": "experimental",
  "jsii": {
    "outdir": "dist",
    "versionFormat": "full",
    "targets": {
      "java": {
        "package": "com.github.toricls.cdk8ssimplewebapp",
        "maven": {
          "groupId": "com.github.toricls",
          "artifactId": "cdk8s-simple-webapp"
        }
      },
      "python": {
        "distName": "cdk8s-simple-webapp",
        "module": "cdk8s_simple_webapp"
      },
      "dotnet": {
        "namespace": "Toricls.Cdk8sSimpleWebapp",
        "packageId": "Tori.Cdk8sSimpleWebapp"
      }
    }
  }
}
